CREATE EXTENSION quack;
-- test numeric types
CREATE TABLE t (
  si smallint,
  i integer,
  bi bigint,
  deci decimal(12,10),
  n numeric(8,5),
  r real,
  dbl double precision,
  ss smallserial,
  s serial,
  bs bigserial
);
